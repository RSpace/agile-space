{
  "name": "agile-space",
  "version": "1.0.0",
  "description": "Agile tools for AltspaceVR",
  "dependencies": {
    "aframe": "0.3.0",
    "aframe-altspace-component": "https://github.com/AltspaceVR/aframe-altspace-component",
    "aframe-react": "2.0.0",
    "altspace": "0.19.0",
    "babel-polyfill": "^6.9.0",
    "immutable": "^3.8.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "three": "^0.73.0",
    "url": "^0.11.0"
  },
  "scripts": {
    "build": "./scripts/build.sh",
    "build-js": "webpack -p --define process.env.NODE_ENV='\"production\"'",
    "serve": "twistd -n web --path ./public -p 8081",
    "serve-js": "webpack-dev-server -d --define process.env.NODE_ENV='\"development\"' --content-base src --progress --watch --history-api-fallback --inline --hot --port 8082 --host 0.0.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rspace/agile-space.git"
  },
  "keywords": [
    "aframe",
    "react",
    "vr",
    "a-frame",
    "mozvr",
    "webvr"
  ],
